@model EmployeeViewModel

@{
    ViewData["Title"] = "Delete Employee";
}

<h5 class="fa fa-warning alert alert-warning">Are You Sure To Delete This Employee?</h5>
<br />
<br />

<form asp-action="Delete">
    <div asp-validation-summary="ModelOnly"></div>

    <partial name="PartialViews/DetailsDeleteEmployeePartial"/>

    <input type="submit" value="Delete" class="btn btn-danger" />
    <a asp-action="Index" class="btn btn-link">Cancel</a>

</form>
